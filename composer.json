{
  "name": "rougin/dextra",
  "description": "\"Ready-to-eat\" CRUD for frontend.",
  "homepage": "https://github.com/rougin/dextra",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "name": "Rougin Gutib"
    }
  ],
  "require":
  {
    "php": ">=5.3.0",
    "tedivm/jshrink": "~1.0"
  },
  "require-dev":
  {
    "phpunit/phpunit": "*",
    "rougin/fortem": "dev-master",
    "rougin/gable": "dev-master",
    "sanmai/phpunit-legacy-adapter": "*"
  },
  "autoload":
  {
    "psr-4":
    {
      "Rougin\\Dextra\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Rougin\\Dextra\\": "tests"
    }
  },
  "scripts":
  {
    "analyze": "phpstan analyse",
    "restyle": "php-cs-fixer fix --config=phpstyle.php",
    "test": "phpunit"
  }
}
